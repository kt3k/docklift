{
  "name": "docklift",
  "version": "0.4.1",
  "description": "Docker's Scripting Interface",
  "main": "lib/index.js",
  "bin": {
    "docklift": "bin/docklift.js"
  },
  "scripts": {
    "test": "_mocha -t 10000 --require babel-register spec/**/*",
    "test-stop": "babel-node casual-tests/stop-container.js",
    "cov": "nyc --reporter=lcov npm test",
    "docker-rm-all": "docker rm -f `docker ps -aq`",
    "lint": "souffle",
    "clean": "rimraf lib coverage",
    "dist": "babel src -d lib",
    "prepublish": "npm-run-all clean dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kt3k/docklift.git"
  },
  "keywords": [
    "docker",
    "container",
    "docklift"
  ],
  "author": "Yoshiya Hinosawa <stibium121@gmail.com> (https://twitter.com/kt3k)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kt3k/docklift/issues"
  },
  "homepage": "https://github.com/kt3k/docklift#readme",
  "dependencies": {
    "chalk": "^1.1.1",
    "dockerode": "^2.2.9",
    "es6-promise": "^3.1.2",
    "interpret": "^1.0.0",
    "liftoff": "^2.2.0",
    "minimist": "^1.2.0",
    "orchestrator": "^0.3.7"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "mocha": "^2.4.5",
    "npm-run-all": "^1.5.2",
    "nyc": "^6.1.1",
    "rimraf": "^2.5.2",
    "souffle-cli": "^3.1.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "souffle": {
    "indent": 2,
    "globals": [
      "describe",
      "it"
    ]
  },
  "nyc": {
    "exclude": [
      "spec"
    ]
  }
}
